   "source": [
    "def grad(x):\n",
    "    x = x.clone()\n",
    "    x.requires_grad_(True)\n",
    "    f = torch.max(torch.abs(x))\n",
    "    f.backward()\n",
    "    return x.grad"
   ]
  },
